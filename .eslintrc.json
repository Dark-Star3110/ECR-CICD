{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "brace-style": ["error"],
    "no-unused-vars": ["warn"],
    "no-undef": "off",
    "no-var": ["off"],
    "one-var": ["off"]
  }
}
